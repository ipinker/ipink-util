{
	"name": "ipink-util",
	"version": "1.4.1",
	"description": "util.js",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		},
		"./es": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs"
		},
		"./lib": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.js"
		},
		"./*": "./*"
	},
	"keywords": [
		"javascript",
		"uniapp",
		"util",
		"utiljs",
		"ipink"
	],
	"files": [
		"dist",
		"package.json",
		"README.md",
		"global.d.ts",
		"LICENSE"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/ipinker/ipink-util.git"
	},
	"scripts": {
		"build": "tsc --noEmit && vite build",
        "build:test": "tsc --noEmit && vite build --mode test",
		"preview": "vite preview"
	},
	"dependencies": {
		"@ctrl/tinycolor": "^4.0.2",
		"@dcloudio/types": "^3.4.8",
		"ipink-themejs": "^1.0.2",
		"lodash-es": "^4.17.21"
	},
	"license": "ISC"
}
